{
  "name": "app-start-pipeline",
  "version": "1.0.0",
  "description": "Application Pipeline Starter",
  "main": "gulpfile.js",
  "scripts": {
    "jspm": "jspm",
    "clean": "gulp clean -all",
    "watch": "chokidar-socket-emitter -l 9080",
    "test": "cross-env profile=test gulp test",
    "dev": "cross-env profile=dev gulp dev",
    "prod": "cross-env profile=prod gulp prod",
    "release": "cross-env profile=prod gulp release"
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "keywords": [
    "pipeline",
    "systemjs",
    "typescript",
    "react",
    "redux"
  ],
  "author": "Patricio Ferreira <3dimentionar@gmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/bootstrap": "3.3.34",
    "@types/chai": "4.0.1",
    "@types/enzyme": "2.8.1",
    "@types/jquery": "3.2.5",
    "@types/mocha": "2.2.41",
    "@types/react": "15.0.33",
    "@types/sinon": "2.3.2",
    "argsparser": "0.0.7",
    "chai": "4.0.2",
    "chokidar-socket-emitter": "0.6.0",
    "cross-env": "5.0.1",
    "extend": "3.0.1",
    "fs-extra": "3.0.1",
    "gulp": "3.9.1",
    "jspm": "0.16.53",
    "karma": "1.7.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sinon": "1.0.5",
    "karma-spec-reporter": "0.0.31",
    "karma-systemjs": "github:adamwillden/karma-systemjs#systemjs-0.20",
    "lite-server": "2.3.0",
    "mocha": "3.4.2",
    "plugin-typescript": "7.0.6",
    "pug-cli": "git+ssh://git@github.com/pugjs/pug-cli#master",
    "remap-istanbul": "0.9.5",
    "run-sequence": "2.0.0",
    "sinon": "2.3.6",
    "typescript": "2.4.1",
    "underscore": "1.8.3",
    "underscore.string": "3.3.4"
  },
  "environment": {
    "test": {
      "dependencies": {
        "bundles": "libraries + libraries-dev + libraries-test",
        "output": "src/js/libraries/libraries-test.js",
        "config": {
          "defaultJSExtensions": true,
          "map": {
            "plugin-babel": "jspm/npm/systemjs-plugin-babel@0.0.25.js",
            "systemjs-babel-build": "jspm/npm/systemjs-plugin-babel@0.0.25/systemjs-babel-browser.js"
          },
          "transpiler": "plugin-babel"
        },
        "options": {
          "mangle": false,
          "normalize": true
        }
      }
    },
    "dev": {
      "server": {
        "port": 3002,
        "open": false,
        "logLevel": "warn",
        "files": [
          "**/*.{html,css,js,ts,tsx,json,map,jpg,png,gif}"
        ],
        "snippetOptions": {
          "ignorePaths": "**/*.{ts,tsx}"
        },
        "server": {
          "baseDir": [
            "./",
            "./src",
            "./dist"
          ]
        }
      },
      "dependencies": {
        "bundles": "libraries + libraries-dev",
        "output": "src/js/libraries/libraries-dev.js",
        "config": {
          "defaultJSExtensions": true,
          "map": {
            "plugin-babel": "jspm/npm/systemjs-plugin-babel@0.0.25.js",
            "systemjs-babel-build": "jspm/npm/systemjs-plugin-babel@0.0.25/systemjs-babel-browser.js"
          },
          "transpiler": "plugin-babel"
        },
        "options": {
          "mangle": false,
          "normalize": true
        }
      }
    },
    "prod": {
      "server": {
        "port": 3002,
        "open": false,
        "logLevel": "warn",
        "files": [
          "**/*.{html,css,js,json,map,jpg,png,gif}"
        ],
        "server": {
          "baseDir": [
            "./dist"
          ]
        }
      },
      "dependencies": {
        "bundles": "libraries",
        "output": "src/js/libraries/libraries.js",
        "config": {
          "defaultJSExtensions": true,
          "transpiler": false
        },
        "options": {
          "minify": true,
          "mangle": true,
          "normalize": true
        }
      },
      "code": {
        "config": {
          "defaultJSExtensions": true,
          "transpiler": "ts",
          "typescriptOptions": {
            "tsconfig": true
          },
          "paths": {
            "typescript": "./node_modules/typescript",
            "ts": "./node_modules/plugin-typescript/lib/plugin.js",
            "tsconfig.json": "./tsconfig.json",
            "bootstrap-css/": "jspm/github/twbs/bootstrap@3.3.7/css/"
          },
          "map": {
            "css": "jspm/github/systemjs/plugin-css@0.1.35",
            "less": "jspm/npm/systemjs-less-plugin@2.0.0"
          },
          "packages": {
            "typescript": {
              "main": "lib/typescript.js",
              "meta": {
                "lib/typescript.js": {
                  "exports": "ts"
                }
              }
            },
            "css": {
              "main": "css.js"
            },
            "less": {
              "main": "less.js"
            },
            "bundles": {
              "defaultExtension": "tsx",
              "meta": {
                "/*": {
                  "format": "register",
                  "loader": "ts"
                }
              }
            },
            "pages": {
              "defaultExtension": "tsx",
              "meta": {
                "/*": {
                  "format": "register",
                  "loader": "ts"
                }
              }
            },
            "ui": {
              "defaultExtension": "tsx",
              "meta": {
                "/*": {
                  "format": "register",
                  "loader": "ts"
                }
              }
            },
            "store": {
              "defaultExtension": "ts",
              "meta": {
                "/*": {
                  "format": "register",
                  "loader": "ts"
                }
              }
            },
            "service": {
              "defaultExtension": "ts",
              "meta": {
                "/*": {
                  "format": "register",
                  "loader": "ts"
                }
              }
            },
            "util": {
              "defaultExtension": "ts",
              "meta": {
                "/*": {
                  "format": "register",
                  "loader": "ts"
                }
              }
            }
          }
        },
        "options": {
          "minify": true,
          "mangle": true,
          "format": "esm",
          "normalize": true,
          "externals": [
            "bluebird",
            "bootstrap",
            "jquery",
            "react",
            "react-dom",
            "redux",
            "immutable",
            "redux-actions",
            "redux-promise",
            "redux-socket.io",
            "socket.io-client"
          ]
        }
      }
    }
  },
  "jspm": {
    "directories": {
      "baseURL": "src/js",
      "format": "esm",
      "packages": "src/js/jspm"
    },
    "configFile": "src/jspm.js",
    "dependencies": {
      "bluebird": "npm:bluebird@3.5.0",
      "bootstrap": "github:twbs/bootstrap@3.3.7",
      "css": "github:systemjs/plugin-css@0.1.35",
      "immutable": "npm:immutable@3.8.1",
      "jquery": "npm:jquery@3.2.1",
      "less": "npm:systemjs-less-plugin@2.0.0",
      "plugin-babel": "npm:systemjs-plugin-babel@0.0.25",
      "react": "npm:react@15.6.1",
      "react-dom": "npm:react-dom@15.6.1",
      "redux": "npm:redux@3.7.1",
      "redux-actions": "npm:redux-actions@2.2.1",
      "redux-logger": "npm:redux-logger@3.0.6",
      "redux-promise": "npm:redux-promise@0.5.3",
      "redux-socket.io": "npm:redux-socket.io@1.4.0",
      "socket.io-client": "npm:socket.io-client@2.0.3"
    },
    "devDependencies": {
      "enzyme": "npm:enzyme@2.9.1",
      "react-addons-test-utils": "npm:react-addons-test-utils@15.6.0",
      "react-test-renderer": "npm:react-test-renderer@15.6.1",
      "systemjs-hot-reloader": "npm:systemjs-hot-reloader@1.1.0"
    },
    "overrides": {
      "npm:enzyme@2.9.1": {
        "map": {
          "react/addons": "@empty",
          "react/lib/ExecutionEnvironment": "@empty",
          "react/lib/ReactContext": "@empty"
        }
      }
    }
  }
}
